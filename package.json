{
  "name": "layaair-tape",
  "main": "./dist/tape.js",
  "version": "4.9.1",
  "description": "A layaair game library",
  "scripts": {
    "sutep": "npm i && cd library && npm i",
    "start": "cd library && npm run start",
    "build": "cd library && npm run build",
    "test-create": "node ./bin/build.js create test",
    "test-server": "node ./bin/build.js server",
    "test-build-h5": "node ./bin/build.js build --platform h5 --output ./build/ --input ./test/bin/ --force --min",
    "test-build-cocos": "node ./bin/build.js build --platform quickgame --output ./build/ --input ./test/bin/ --force --zip --zip-name game.cpk",
    "test-build-android": "node ./bin/build.js build --platform android --output ./android/ --input ./test/bin/ --force"
  },
  "author": "liangmayong",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoopschat/layaair-tape.git"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "keywords": [
    "tape",
    "layaair",
    "layaair-tape"
  ],
  "bin": {
    "layaair-tape": "./bin/build.js"
  },
  "files": [
    "tpl",
    "bin",
    "dist",
    "types"
  ],
  "dependencies": {
    "@babel/core": "7.10.3",
    "babel-preset-env": "1.7.0",
    "cheerio": "1.0.0-rc.2",
    "del": "3.0.0",
    "download": "^8.0.0",
    "exec-sh": "0.3.2",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0",
    "gulp-cheerio": "0.6.3",
    "gulp-concat": "2.6.1",
    "gulp-html-img64": "1.0.8",
    "gulp-minify-css": "1.2.4",
    "gulp-pngquant": "1.0.12",
    "gulp-replace": "1.0.0",
    "gulp-uglify": "3.0.1",
    "gulp-zip": "4.2.0",
    "http-server": "^0.12.3",
    "iconv-lite": "0.4.24",
    "layadcc": "1.8.3",
    "minimist": "1.2.0"
  }
}
